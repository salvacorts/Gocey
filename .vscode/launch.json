    "version": "0.2.0",
    "configurations": [
    {
        "name": "Launch Server",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${workspaceFolder}/mlp-ea-decentralized/native/server",
        "args": [
            "-grpcPort", "1234",
            "-clusterPort", "6666",
            "-clusterBoostrap", "127.0.0.1:9999",
        ]
    },
    {
        "name": "Launch Client",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${workspaceFolder}/mlp-ea-decentralized/native/client",
        "args": [
            "-server", "127.0.0.1:1234",
        ]
    }
    ]
}